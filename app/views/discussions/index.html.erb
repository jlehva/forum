<div class="container">
  <h1>Discussions</h1>

  <% if current_user %>
      <div>
        <%= link_to new_discussion_path, class: 'btn btn-default'  do %>
            <span class="glyphicon glyphicon-file"></span> New Discussion
        <% end %>
        <p></p>
      </div>
  <% end %>

  <% @discussions.each do |discussion| %>
      <%= render 'discussions/postLink', discussion: discussion %>
  <% end %>

  <br>

</div>